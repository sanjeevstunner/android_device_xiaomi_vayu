/data/vendor/fpdump(/.*)?                                            u:object_r:fingerprint_data_file:s0
/data/vendor/goodix(/.*)?                                            u:object_r:fingerprint_data_file:s0

# HALs
/vendor/bin/hw/android\.hardware\.biometrics\.fingerprint@2.1-service\.vayu u:object_r:hal_fingerprint_default_exec:s0
/vendor/bin/hw/android\.hardware\.lights-service\.vayu               u:object_r:hal_light_default_exec:s0

